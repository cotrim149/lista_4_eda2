{
    this->startTime = startTime;
}

vector<Job> createRandomTasks(){

	srand (static_cast <unsigned> (time(0)));

	int totalTasks = 10;
	vector<Job> jobs(totalTasks);


	for(int i=0;i<totalTasks;i++){
		Job job;
		float r2 = static_cast <float> (rand()) / (static_cast <float> (RAND_MAX/X));

	}



	return jobs;


}




